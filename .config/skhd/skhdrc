# app shortcuts
hyper - t : open -a 'kitty'
hyper - s : open -a 'Spotify'
hyper - f : open -a 'Finder'
hyper - k : open -a 'Slack'
hyper - 0x27 : open -a 'ChatWise' # 0x27 = '

# chrome
meh - m [
    "Google Chrome" : ~/.config/skhd/scripts/chrome-merge-windows.js
]
meh - d [
    "Google Chrome" : ~/.config/skhd/scripts/chrome-duplicate-tab.js
]

# finder
meh - c [
    "Finder" : ~/.config/skhd/scripts/finder-copy-path.js
]
