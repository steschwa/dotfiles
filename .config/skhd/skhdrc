# chrome
cmd - m [
    "Google Chrome" : osascript /Users/stefan/.config/macos/chrome-merge-windows.scpt
]
cmd - d [
    "Google Chrome" : osascript /Users/stefan/.config/macos/chrome-duplicate-tab.scpt
]

# finder
cmd + shift - c [
    "Finder" : osascript /Users/stefan/.config/macos/finder-copy-path.scpt
]

# app shortcuts
hyper - t : osascript -e 'tell application "WezTerm" to activate'
hyper - s : osascript /Users/stefan/.config/macos/open-spotify.scpt
