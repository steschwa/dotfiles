# app shortcuts
hyper - t : open -a 'WezTerm'
hyper - s : open -a 'Spotify'
hyper - f : open -a 'Finder'
hyper - b : open -a 'Bear'
hyper - g : open -a 'Fork'

# chrome
cmd - m [
    "Google Chrome" : ~/.config/macos/chrome-merge-windows.js
]
cmd - d [
    "Google Chrome" : ~/.config/macos/chrome-duplicate-tab.js
]

# finder
cmd + shift - c [
    "Finder" : ~/.config/macos/finder-copy-path.js
]
